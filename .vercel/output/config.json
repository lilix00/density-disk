{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/about/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/webhook/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/folder/?$",
			"dest": "_render"
		},
		{
			"src": "^/news/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/news/?$",
			"dest": "_render"
		},
		{
			"src": "^/posts/post-1/?$",
			"dest": "_render"
		},
		{
			"src": "^/posts/post-2/?$",
			"dest": "_render"
		},
		{
			"src": "^/posts/post-3/?$",
			"dest": "_render"
		},
		{
			"src": "^/posts/post-4/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}